#include <iostream>
using namespace std;
int main()
{
    int A,B;
    cout<<"Ingrese el valor de A: ";
    cin>>A;
    cout<<"Ingrese el valor de B: ";
    cin>>B;
    cout<<"Los valores de A y B antes del intercambio son: "<<A<<" y "<<B<<endl;
    A=A+B;
    B=A-B;
    A=A-B;
    cout<<"Los valores intercambiados son: "<<A<<" y "<<B;

    return 0;   
}
