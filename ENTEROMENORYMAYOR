#include <iostream>
using namespace std;
int main()
{
    int n1,n2,n3,n4,n5, mayor,menor;
    cout<<"Ingrese el primer numero entero: ";
    cin>>n1;
    mayor=n1;
    menor=n1;
    cout<<"Ingrese el segundo numero entero: ";
    cin>>n2;
    if (mayor<n2)
        mayor=n2;
    if (menor>n2)
        menor=n2;
    cout<<"Ingrese el tercer numero entero: ";
    cin>>n3;
    if (mayor<n3)
        mayor=n3;
    if (menor>n3)
        menor=n3;
    cout<<"Ingrese el cuarto numero entero: ";
    cin>>n4;
    if (mayor<n4)
        mayor=n4;
    if (menor>n4)
        menor=n4;
    cout<<"Ingrese el quinto numero entero: ";
    cin>>n5;
    if (mayor<n5)
        mayor=n5;
    if (menor>n5)
        menor=n5;
    cout<<"El numero mayor es "<<mayor<<" y el menor es "<<menor;
    
    return 0;   
}
